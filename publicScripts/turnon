#!/usr/local/bin/nodejs

let net = require('net')

const serverAddress = "192.168.28.35"

function send(host, port, message) {
  const socket = new net.Socket()
  let responses = []

  // Send message after connection
  socket.connect({port, host}, () => {
      socket.end(message)
  })
}

send(serverAddress, 20000, '*1*1*13##')